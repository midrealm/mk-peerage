<div class="container">
<div class="col-md-3">
<h1>Edit Profile</h1>
<%= form_for @user, as: :user, url: users_edit_path do |f| %>
  <div class='form-group'>
    <%= f.label :sca_name, 'SCA Name' %>
    <%= f.text_field :sca_name, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :modern_name, 'Modern Name' %>
    <%= f.text_field :modern_name, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :street, 'Street' %>
    <%= f.text_field :street, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :city  %>
    <%= f.text_field :city, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :state  %>
    <%= f.text_field :state, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :phone, 'Phone Number' %>
    <%= f.telephone_field :phone, class:'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.label :eleveators, 'Elevated By' %>
    <%= f.text_field :elevators, class:'form-control' %>
  </div>

  <%= f.submit class: 'btn btn-default'%>
<% end %>
</div>

</div>
