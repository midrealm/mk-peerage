<h1>Manage Laurels</h1>
<%= link_to 'Add New Laurel', new_admin_laurel_path %>
<table class='table'>
  <thead>
    <th>Name</th><th>Show</th><th>Edit</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
      <td><%=user.sca_name%></td>
      <td><%=link_to 'Show', user.url%></td>
      <td><%=link_to 'Edit', edit_admin_laurel_path(user.id)%></td>
      </tr>
    <% end %>
  </tbody
</table>
