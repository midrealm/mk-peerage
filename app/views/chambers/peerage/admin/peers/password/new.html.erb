<% title "Send Reset Password Email"%>
<div class="row justify-content-center">
  <div class="col-md-6">
<h1>Send Reset Password Email</h2>

<%= bootstrap_form_tag url: chambers_admin_peers_password_path do |f| %>
  <%= f.select :id, @peers, {label: 'Peers'}, {class: 'selectpicker', data: {'live-search' => true}} %>
  <%= f.submit "Send Reset Password Info", class: 'btn btn-secondary btn-block' %>
<% end %>
</div>
</div>
